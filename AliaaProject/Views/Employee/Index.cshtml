                @Html.DisplayFor(modelItem => item.IsObserving)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.College.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Governorate.Id)
            </td>
