                @Html.DisplayFor(modelItem => item.University.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NationalId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NationalId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NationalId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
